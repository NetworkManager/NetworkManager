"                  [mac <MAC-Adresse>]\n"
"                  [cloned-mac <geklonte MAC-Adresse>]\n"
"                  [mtu <MTU>]\n"
"                  [mode infrastructure|ap|adhoc]\n"
"\n"
"    wimax:        [mac <MAC-Adresse>]\n"
"                  [nsp <NSP>]\n"
"\n"
"    pppoe:        username <PPPoE-Benutzername>\n"
"                  [password <PPPoE-Passwort>]\n"
"                  [service <PPPoE-Dienstname>]\n"
"                  [mtu <MTU>]\n"
"                  [mac <MAC-Adresse>]\n"
"\n"
