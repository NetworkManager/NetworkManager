"                  [user <Benutzername>]\n"
"                  [password <Passwort>]\n"
"\n"
"    cdma:         [user <Benutzername>]\n"
"                  [password <Passwort>]\n"
"\n"
"    infiniband:   [mac <MAC-Adresse>]\n"
"                  [mtu <MTU>]\n"
"                  [transport-mode datagram | connected]\n"
"                  [parent <Schnittstellenname>]\n"
